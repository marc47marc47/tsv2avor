{
  "configurations": [
    {
      "type": "cppdbg",
      "request": "launch",
      "name": "Launch tsv2avor",
      "program": "${workspaceFolder}/target/debug/${input:executableName}",
      "args": [
        "${workspaceFolder}/sql_logs.tsv",
        "${workspaceFolder}/sql_logs.avor"
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}"
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "executableName",
      "description": "Name of your executable"
    }
  ]
}
